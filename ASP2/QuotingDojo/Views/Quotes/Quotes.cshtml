@model Quote
<div class="jumbotron">
    <h1 class="display-4">All Quotes</h1>
</div>

<main>
    <ul>
    @foreach(var quote in ViewBag.Quotes)
    {
        <li class="mt-3">"@quote["Content"]"</li>
        <li class="border-bottom ml-3"> - @quote["User"] at @quote["Created_At"]</li>
    }
    </ul>
</main>