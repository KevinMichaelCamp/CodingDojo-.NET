@model Wedding

<partial name="NavPartial"/>
<div class="jumbotron">
    <h1 class="display-4">Plan Wedding</h1>
</div>
<form asp-action="Create" asp-controller="Home" method="post">
    <table class="table table-borderless">
        <tr>
            <td><label asp-for="Groom"></label></td>
            <td><input class="form-control" asp-for="Groom"></td>
        </tr>
        <tr>
            <td><label asp-for="Bride"></label></td>
            <td><input class="form-control" asp-for="Bride"></td>
        </tr>
        <tr>
            <td><label asp-for="Date"></label></td>
            <td><input class="form-control" asp-for="Date"></td>
        </tr>
        <tr>
            <td><label asp-for="Address"></label></td>
            <td><input class="form-control" asp-for="Address"></td>
        </tr>
    </table>
    <button class="btn btn-primary" type="submit">Create</button>
</form>
<br>
<span asp-validation-for="Groom"></span>
<br>
<span asp-validation-for="Bride"></span>
<br>
<span asp-validation-for="Date"></span>
<br>
<span asp-validation-for="Address"></span>
